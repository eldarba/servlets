<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">
	<display-name>targil1</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<!-- define the date servlet -->
	<servlet>
		<servlet-name>dates</servlet-name>
		<servlet-class>a.DatesSrvlet</servlet-class>
	</servlet>
	
	<!-- define the date servlet url -->
	<servlet-mapping>
		<servlet-name>dates</servlet-name>
		<url-pattern>/dates</url-pattern>	
	</servlet-mapping>
	
	<!-- define the date servlet -->
	<servlet>
		<servlet-name>rectangle</servlet-name>
		<servlet-class>a.Rectangle</servlet-class>
	</servlet>
	
	<!-- define the date servlet url -->
	<servlet-mapping>
		<servlet-name>rectangle</servlet-name>
		<url-pattern>/rectangle</url-pattern>	
	</servlet-mapping>

</web-app>