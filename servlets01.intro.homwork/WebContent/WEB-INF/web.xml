<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>servlets01.intro.homwork</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>date servlet</servlet-name>
    <servlet-class>a.DateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>date servlet</servlet-name>
    <url-pattern>/dates</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>rectangle servlet</servlet-name>
    <servlet-class>a.RectangleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>rectangle servlet</servlet-name>
    <url-pattern>/rectangle</url-pattern>
  </servlet-mapping>
</web-app>